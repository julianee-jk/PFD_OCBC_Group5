@{
    ViewData["Title"] = "HomePage";
}

<div id="main_box">
    <div class="inner_main_box">
        


        <h2><strong>Non-Singpass Holders</strong></h2>
        <h6>Both applicants do not have Singpass</h6>

        <a asp-controller="NonSP" asp-action="PersonInfo" class="btn btn-outline-danger register-btn" id="non-singpass-holder-btn">Register</a>
        <p>This will redirect you to a registration page.</p>

        <a asp-action="Continue" class="btn btn-outline-danger register-btn" id="non-singpass-holder-btn">Continue</a>
        <p>This will redirect you to input mobile number.</p>
    </div>

    <div class="vl"></div>
    <div class="inner_main_box">
        <h2><strong>Singpass Holders</strong></h2>
        <h6>Both applicants have Singpass</h6>

        <a asp-controller="Singpass" asp-action="SingpassLogin" asp-route-currentUser="1" class="btn btn-outline-danger register-btn" id="singpass-holder-btn">Register / Login</a>
        <p>This will redirect you to Singpass link.</p>
        <a asp-controller="Singpass" asp-action="SingpassRegister" class="btn btn-outline-danger register-btn" id="singpass-holder-btn">Register / Login</a>
        <p>This will redirect you to Singpass register link.</p>
    </div>

</div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
